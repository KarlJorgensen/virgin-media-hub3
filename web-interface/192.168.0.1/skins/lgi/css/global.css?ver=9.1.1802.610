	background: #ededed; 
	max-width: 220px;
	font-size: 15px;
	color: #224499;
	padding: 10px;
	font-family: BloomSpeak_normal;
	font-weight: normal;
	padding: 10px;
	border: 1px solid #dbdbdb;
	border-radius:0;
	line-height: 1.2em;
}

.toolTip::after { 
    position: absolute;
	content: "";
	left: -11px;
	top: 0;
	width: 12px;
	height: 32px;
    background: url(images/bubble-arrow-left-top.png) no-repeat;
}

