.wizard_button, .firstInstall_next, .firstInstall_skip, .wizard_next, .wizard_finish, .wizard_apply {
	background:#224499; 
	font-family: BloomSpeak_normal; 
	font-size: 34px; 
	color: #fff; 
	padding: 12px 35px;
	border:1px solid #2c91f7; 
	border-radius:3px;
	cursor:pointer;
	margin-right:5px;
	margin-left:5px;
}
.wizard_button:hover {
	background:#035eba; 
	border:1px solid #00aadd; 
	box-shadow: 0 0 4px #66bddc;
}
.wizard_button.disabled {
	background:#aaaaaa; 
	font-size: 34px; 
	color: #c2c2c2; 
	border:1px solid #dedede; 
	box-shadow: none;
}

.wizard_back, .wizard_close {
	font-size: 34px; 
	color: #224499; 
	padding: 12px 35px;
	border:1px solid #d2d2d2; 
	border-radius:3px;
	cursor:pointer;
	margin-right:5px;
	margin-left:5px;
}

